{
    "name": "vendors/csv",
    "description": "CSV builder",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.devsms.com/packages.json"
        }
    ],
    "require": {
        "smsapi/valueobjects": "master@dev",
        "rolebi/composer-dependencies-security-checker": "~1.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.4.*",
        "phpunit/phpunit": "4.5.*",
        "phploc/phploc": "~2.0",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "~2.2",
        "squizlabs/php_codesniffer": "~2.2",
        "sebastian/phpcpd": "~2.0",
        "johnkary/phpunit-speedtrap": "~1.0@dev",
        "mybuilder/phpunit-accelerator": "~1.1",
        "facebook/xhprof": "master@dev"
    },
    "autoload": {
        "psr-4": {
            "Csv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Csv\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd" : [
            "Rolebi\\ComposerDependenciesSecurityChecker\\ScriptHandler::checkForSecurityIssues"
        ],
        "post-install-cmd" : [
            "Rolebi\\ComposerDependenciesSecurityChecker\\ScriptHandler::checkForSecurityIssues"
        ]
    }
}
