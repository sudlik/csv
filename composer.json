{
    "name": "vendors/csv",
    "description": "CSV builder",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.devsms.com/packages.json"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-SPL": "*",
        "ext-filter": "*",
        "ext-pcre": "*",
        "smsapi/valueobjects": "master@dev",
        "ngyuki/composer-curl-plugin": "master@dev"
    },
    "require-dev": {
        "ext-xhprof": "*",
        "mikey179/vfsStream": "1.4.*",
        "phpunit/phpunit": "4.5.*",
        "phploc/phploc": "~2.0",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "~2.2",
        "squizlabs/php_codesniffer": "~2.2",
        "sebastian/phpcpd": "~2.0",
        "johnkary/phpunit-speedtrap": "~1.0@dev",
        "mybuilder/phpunit-accelerator": "~1.1",
        "facebook/xhprof": "master@dev",
        "koriym/printo": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Csv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Csv\\Tests\\": "tests/"
        }
    }
}
